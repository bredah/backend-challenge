 {
	"definitions": {},
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "http://example.com/root.json",
	"type": "array",
	"items": {
		"$id": "#/items",
		"type": "object",
		"required": [
			"userId",
			"id",
			"title"
		],
		"properties": {
			"userId": {
				"$id": "#/items/properties/userId",
				"type": "integer"
			},
			"id": {
				"$id": "#/items/properties/id",
				"type": "integer"
			},
			"title": {
				"$id": "#/items/properties/title",
				"type": "string",
				"pattern": "^(.*)$"
			}
		}
	}
}